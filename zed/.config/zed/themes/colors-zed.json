{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0c0c1c90",
                "editor.background": "#0c0c1c90",
                "editor.foreground": "#c2c2c6",
                "text": "#c2c2c6",
                "text.muted": "#c2c2c670",
                "text.ignored": "#c2c2c640",
                "text.placeholder": "#c2c2c650",
                "ignored": "#c2c2c630",
                "element.hover": "#c2c2c630",
                "ghost_element.hover": "#EE3E1130",
                "ghost_element.selected": "#EE3E1130",
                "ghost_element.active": "#EE3E1160",
                "border": "#0c0c1c",
                "editor.highlighted_line.background": "#EE3E1110",
                "editor.active_line.background": "#EE3E1110",
                "panel.background": "#0c0c1c90",
                "title_bar.background": "#0c0c1c90",
                "title_bar.inactive_background": "#0c0c1c90",
                "status_bar.background": "#0c0c1c90",
                "drop_target.background": "#0c0c1c90",
                "elevated_surface.background": "#0c0c1c",
                "toolbar.background": "#0c0c1c90",
                "tab_bar.background": "#0c0c1c90",
                "tab.inactive_background": "#0c0c1c90",
                "tab.active_background": "#EE3E1130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0c0c1c",
                "scrollbar.thumb.background": "#EE3E11",
                "editor.gutter.background": "#0c0c1c90",
                "terminal.background": "#0c0c1c10",
                "terminal.foreground": "#c2c2c6",
                "terminal.dim_foreground": "#c2c2c6",
                "terminal.bright_foreground": "#c2c2c6",
                "terminal.ansi.black": "#0c0c1c",
                "terminal.ansi.red": "#EE3E11",
                "terminal.ansi.green": "#EE4315",
                "terminal.ansi.yellow": "#EB5D35",
                "terminal.ansi.blue": "#A9716A",
                "terminal.ansi.magenta": "#D56B54",
                "terminal.ansi.cyan": "#7E7C86",
                "terminal.ansi.white": "#c2c2c6",
                "terminal.ansi.bright_black": "#5b5b70",
                "terminal.ansi.bright_red": "#EE3E11",
                "terminal.ansi.bright_green": "#EE4315",
                "terminal.ansi.bright_yellow": "#EB5D35",
                "terminal.ansi.bright_blue": "#A9716A",
                "terminal.ansi.bright_magenta": "#D56B54",
                "terminal.ansi.bright_cyan": "#7E7C86",
                "terminal.ansi.bright_white": "#c2c2c6",
                "modified": "#EE3E11",
                "syntax": {
                    "attribute": {
                        "color": "#EE3E11"
                    },
                    "boolean": {
                      "color": "#EE4315"
                    },
                    "comment": {
                      "color": "#5b5b70"
                    },
                    "comment.doc": {
                      "color": "#5b5b70"
                    },
                    "constant": {
                      "color": "#EE4315"
                    },
                    "function": {
                      "color": "#7E7C86"
                    },
                    "keyword": {
                      "color": "#A9716A"
                    },
                    "number": {
                      "color": "#D56B54"
                    },
                    "operator": {
                      "color": "#A9716A"
                    },
                    "string": {
                      "color": "#EE3E11"
                    },
                    "variable": {
                      "color": "#EE4315"
                    }
                },
                "players": [
                  {
                    "cursor": "#EE3E11",
                    "background": "#0c0c1c",
                    "selection": "#EE3E1130"
                  }
                ]
            }
        }
    ]
}
