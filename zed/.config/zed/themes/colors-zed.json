{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#10101190",
                "editor.background": "#10101190",
                "editor.foreground": "#c3c3c3",
                "text": "#c3c3c3",
                "text.muted": "#c3c3c370",
                "text.ignored": "#c3c3c340",
                "text.placeholder": "#c3c3c350",
                "ignored": "#c3c3c330",
                "element.hover": "#c3c3c330",
                "ghost_element.hover": "#4E797E30",
                "ghost_element.selected": "#4E797E30",
                "ghost_element.active": "#4E797E60",
                "border": "#101011",
                "editor.highlighted_line.background": "#4E797E10",
                "editor.active_line.background": "#4E797E10",
                "panel.background": "#10101190",
                "title_bar.background": "#10101190",
                "title_bar.inactive_background": "#10101190",
                "status_bar.background": "#10101190",
                "drop_target.background": "#10101190",
                "elevated_surface.background": "#101011",
                "toolbar.background": "#10101190",
                "tab_bar.background": "#10101190",
                "tab.inactive_background": "#10101190",
                "tab.active_background": "#4E797E30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#101011",
                "scrollbar.thumb.background": "#4E797E",
                "editor.gutter.background": "#10101190",
                "terminal.background": "#10101110",
                "terminal.foreground": "#c3c3c3",
                "terminal.dim_foreground": "#c3c3c3",
                "terminal.bright_foreground": "#c3c3c3",
                "terminal.ansi.black": "#101011",
                "terminal.ansi.red": "#4E797E",
                "terminal.ansi.green": "#B45B3A",
                "terminal.ansi.yellow": "#D1A641",
                "terminal.ansi.blue": "#3B8388",
                "terminal.ansi.magenta": "#4B8889",
                "terminal.ansi.cyan": "#ACA994",
                "terminal.ansi.white": "#c3c3c3",
                "terminal.ansi.bright_black": "#59596d",
                "terminal.ansi.bright_red": "#4E797E",
                "terminal.ansi.bright_green": "#B45B3A",
                "terminal.ansi.bright_yellow": "#D1A641",
                "terminal.ansi.bright_blue": "#3B8388",
                "terminal.ansi.bright_magenta": "#4B8889",
                "terminal.ansi.bright_cyan": "#ACA994",
                "terminal.ansi.bright_white": "#c3c3c3",
                "modified": "#4E797E",
                "syntax": {
                    "attribute": {
                        "color": "#4E797E"
                    },
                    "boolean": {
                      "color": "#B45B3A"
                    },
                    "comment": {
                      "color": "#59596d"
                    },
                    "comment.doc": {
                      "color": "#59596d"
                    },
                    "constant": {
                      "color": "#B45B3A"
                    },
                    "function": {
                      "color": "#ACA994"
                    },
                    "keyword": {
                      "color": "#3B8388"
                    },
                    "number": {
                      "color": "#4B8889"
                    },
                    "operator": {
                      "color": "#3B8388"
                    },
                    "string": {
                      "color": "#4E797E"
                    },
                    "variable": {
                      "color": "#B45B3A"
                    }
                },
                "players": [
                  {
                    "cursor": "#4E797E",
                    "background": "#101011",
                    "selection": "#4E797E30"
                  }
                ]
            }
        }
    ]
}
