{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0f1c2490",
                "editor.background": "#0f1c2490",
                "editor.foreground": "#c3c6c8",
                "text": "#c3c6c8",
                "text.muted": "#c3c6c870",
                "text.ignored": "#c3c6c840",
                "text.placeholder": "#c3c6c850",
                "ignored": "#c3c6c830",
                "element.hover": "#c3c6c830",
                "ghost_element.hover": "#59577030",
                "ghost_element.selected": "#59577030",
                "ghost_element.active": "#59577060",
                "border": "#0f1c24",
                "editor.highlighted_line.background": "#59577010",
                "editor.active_line.background": "#59577010",
                "panel.background": "#0f1c2490",
                "title_bar.background": "#0f1c2490",
                "title_bar.inactive_background": "#0f1c2490",
                "status_bar.background": "#0f1c2490",
                "drop_target.background": "#0f1c2490",
                "elevated_surface.background": "#0f1c24",
                "toolbar.background": "#0f1c2490",
                "tab_bar.background": "#0f1c2490",
                "tab.inactive_background": "#0f1c2490",
                "tab.active_background": "#59577030",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0f1c24",
                "scrollbar.thumb.background": "#595770",
                "editor.gutter.background": "#0f1c2490",
                "terminal.background": "#0f1c2410",
                "terminal.foreground": "#c3c6c8",
                "terminal.dim_foreground": "#c3c6c8",
                "terminal.bright_foreground": "#c3c6c8",
                "terminal.ansi.black": "#0f1c24",
                "terminal.ansi.red": "#595770",
                "terminal.ansi.green": "#936177",
                "terminal.ansi.yellow": "#1C6C89",
                "terminal.ansi.blue": "#5E6F8B",
                "terminal.ansi.magenta": "#9A7089",
                "terminal.ansi.cyan": "#CB7C83",
                "terminal.ansi.white": "#c3c6c8",
                "terminal.ansi.bright_black": "#5e6b74",
                "terminal.ansi.bright_red": "#595770",
                "terminal.ansi.bright_green": "#936177",
                "terminal.ansi.bright_yellow": "#1C6C89",
                "terminal.ansi.bright_blue": "#5E6F8B",
                "terminal.ansi.bright_magenta": "#9A7089",
                "terminal.ansi.bright_cyan": "#CB7C83",
                "terminal.ansi.bright_white": "#c3c6c8",
                "modified": "#595770",
                "syntax": {
                    "attribute": {
                        "color": "#595770"
                    },
                    "boolean": {
                      "color": "#936177"
                    },
                    "comment": {
                      "color": "#5e6b74"
                    },
                    "comment.doc": {
                      "color": "#5e6b74"
                    },
                    "constant": {
                      "color": "#936177"
                    },
                    "function": {
                      "color": "#CB7C83"
                    },
                    "keyword": {
                      "color": "#5E6F8B"
                    },
                    "number": {
                      "color": "#9A7089"
                    },
                    "operator": {
                      "color": "#5E6F8B"
                    },
                    "string": {
                      "color": "#595770"
                    },
                    "variable": {
                      "color": "#936177"
                    }
                },
                "players": [
                  {
                    "cursor": "#595770",
                    "background": "#0f1c24",
                    "selection": "#59577030"
                  }
                ]
            }
        }
    ]
}
